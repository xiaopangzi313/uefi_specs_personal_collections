DEFAULT REL
SECTION .text

;------------------------------------------------------------------------------
; void
; __cdecl
; AsmSetDataSelectors (
;    UINT16   SelectorValue
;   );
;------------------------------------------------------------------------------
global AsmSetDataSelectors
AsmSetDataSelectors:
o16 mov     ds, cx
o16 mov     es, cx
o16 mov     fs, cx
o16 mov     gs, cx
o16 mov     ss, cx
  ret

