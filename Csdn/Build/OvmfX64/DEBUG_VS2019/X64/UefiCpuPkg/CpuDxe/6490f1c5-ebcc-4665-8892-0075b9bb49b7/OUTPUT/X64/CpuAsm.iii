    DEFAULT REL
    SECTION .text

;------------------------------------------------------------------------------
; void
; SetCodeSelector (
;   UINT16 Selector
;   );
;------------------------------------------------------------------------------
global SetCodeSelector
SetCodeSelector:
    push    rcx
    lea     rax, [setCodeSelectorLongJump]
    push    rax
    retfq
setCodeSelectorLongJump:
    ret

;------------------------------------------------------------------------------
; void
; SetDataSelectors (
;   UINT16 Selector
;   );
;------------------------------------------------------------------------------
global SetDataSelectors
SetDataSelectors:
o16 mov     ss, cx
o16 mov     ds, cx
o16 mov     es, cx
o16 mov     fs, cx
o16 mov     gs, cx
    ret

