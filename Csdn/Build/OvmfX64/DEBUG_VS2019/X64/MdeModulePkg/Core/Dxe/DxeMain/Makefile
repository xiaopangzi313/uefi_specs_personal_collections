	-@if exist $(BIN_DIR)\MdeModulePkg\Library\LzmaCustomDecompressLib\LzmaCustomDecompressLib\Makefile "$(MAKE)" $(MAKE_FLAGS) -f $(BIN_DIR)\MdeModulePkg\Library\LzmaCustomDecompressLib\LzmaCustomDecompressLib\Makefile cleanall
	@cd $(MODULE_BUILD_DIR)

