    DEFAULT REL
    SECTION .text

;------------------------------------------------------------------------------
;  void *
;  __cdecl
;  InternalMemSetMem32 (
;     void   *Buffer,
;     UINTN  Count,
;     UINT32 Value
;    );
;------------------------------------------------------------------------------
global InternalMemSetMem32
InternalMemSetMem32:
    push    rdi
    mov     rdi, rcx
    mov     rax, r8
    xchg    rcx, rdx
    rep     stosd
    mov     rax, rdx
    pop     rdi
    ret

