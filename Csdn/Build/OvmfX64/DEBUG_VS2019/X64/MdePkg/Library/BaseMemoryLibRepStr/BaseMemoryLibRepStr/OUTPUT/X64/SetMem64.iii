    DEFAULT REL
    SECTION .text

;------------------------------------------------------------------------------
;  void *
;  InternalMemSetMem64 (
;     void   *Buffer,
;     UINTN  Count,
;     UINT64 Value
;    )
;------------------------------------------------------------------------------
global InternalMemSetMem64
InternalMemSetMem64:
    push    rdi
    mov     rdi, rcx
    mov     rax, r8
    xchg    rcx, rdx
    rep     stosq
    mov     rax, rdx
    pop     rdi
    ret

